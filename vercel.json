{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/download",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}